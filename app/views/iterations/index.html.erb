<h1>Listing iterations</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Name</th>
    <th>Start date</th>
    <th>Exit date</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iterations.each do |iteration| %>
  <tr>
    <td><%= iteration.project_id %></td>
    <td><%= iteration.name %></td>
    <td><%= iteration.start_date %></td>
    <td><%= iteration.exit_date %></td>
    <td><%= iteration.status %></td>
    <td><%= link_to 'Show', iteration %></td>
    <td><%= link_to 'Edit', edit_iteration_path(iteration) %></td>
    <td><%= link_to 'Destroy', iteration, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Iteration', new_iteration_path %>
