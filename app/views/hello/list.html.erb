<table border="1">
  <tr>
    <th>category</th><th>content</th>
    <th>iteration_id</th><th>created_at</th>
    <th>hit</th><th>updated_at</th>
  </tr>

<% @kpts.each do |kpt|%>
  <tr>
    <td><%= kpt.category %></td>
    <td><%= kpt.content %></td>
    <td><%= kpt.iteration_id %></td>
    <td><%= kpt.created_at %></td>
    <td><%= kpt.hit %></td>
    <td><%= kpt.updated_at %></td>
  </tr>
<% end %>
</table>
